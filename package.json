{
  "name": "onlinewebgame",
  "version": "1.0.0",
  "description": "Online web game project.",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "tsc": "tsc",
    "cphtml": "cp -r src/html/* dist/",
    "cpjson": "cp -r src/jsondata/ dist/",
    "bsify": "browserify src/static/game.ts -p [ tsify --noImplicitAny ] > dist/static/game.js",
    "retjson": "cp -r dist/jsondata/ src/",
    "build": "npm-run-all retjson clean cphtml cpjson tsc bsify",
    "dev:start": "npm-run-all build start",
    "start": "node dist/server.js"
  },
  "author": "Ab Aeterno",
  "license": "ISC",
  "dependencies": {
    "@types/express-session": "^1.15.12",
    "@types/express-socket.io-session": "^1.3.1",
    "@types/socket.io-client": "^1.4.32",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "express-socket.io-session": "^1.3.5",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "tsify": "^4.0.1",
    "typescript": "^3.4.2",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.16.1",
    "@types/shelljs": "^0.8.3",
    "@types/socket.io": "^2.1.2"
  }
}
