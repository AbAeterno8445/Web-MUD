{
  "name": "onlinewebgame",
  "version": "1.0.0",
  "description": "Online web game project.",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "tsc": "tsc",
    "cphtml": "cp -r src/html/* dist/",
    "cpjson": "cp -r src/jsondata/ dist/",
    "retjson": "cp -r dist/jsondata/ src/",
    "build": "npm-run-all retjson clean cphtml cpjson tsc",
    "dev:start": "npm-run-all build start",
    "start": "node dist/server.js"
  },
  "author": "Ab Aeterno",
  "license": "ISC",
  "dependencies": {
    "@types/express-session": "^1.15.12",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "google-auth-library": "^3.1.2",
    "socket.io": "^2.2.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.16.1",
    "@types/shelljs": "^0.8.3",
    "@types/socket.io": "^2.1.2"
  }
}
